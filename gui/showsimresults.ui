<!DOCTYPE UI><UI version="3.1" stdsetdef="1">
<class>ShowSimResultsForm</class>
<widget class="QDialog">
    <property name="name">
        <cstring>ShowSimResultsForm</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>512</width>
            <height>380</height>
        </rect>
    </property>
    <property name="caption">
        <string>Simulation Results</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>groupBox1</cstring>
            </property>
            <property name="title">
                <string>Haplotypes:</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QTable">
                    <property name="name">
                        <cstring>i_haplotype_table</cstring>
                    </property>
                    <property name="numRows">
                        <number>3</number>
                    </property>
                    <property name="numCols">
                        <number>3</number>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout4</cstring>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QPushButton" row="1" column="2">
                    <property name="name">
                        <cstring>pushButton3</cstring>
                    </property>
                    <property name="text">
                        <string>Close</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="0" column="0">
                    <property name="name">
                        <cstring>mode</cstring>
                    </property>
                    <property name="text">
                        <string>Print leaves only</string>
                    </property>
                    <property name="checked">
                        <bool>true</bool>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Selects whether the entire recomgination graph should be printed, or only the leaves</string>
                    </property>
                </widget>
                <widget class="QPushButton" row="0" column="2">
                    <property name="name">
                        <cstring>saveTextButton</cstring>
                    </property>
                    <property name="text">
                        <string>Save Text</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Run the simulation</string>
                    </property>
                </widget>
                <widget class="QPushButton" row="0" column="1">
                    <property name="name">
                        <cstring>saveXMLButton</cstring>
                    </property>
                    <property name="text">
                        <string>Save XML</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Run the simulation</string>
                    </property>
                </widget>
            </grid>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>saveXMLButton</sender>
        <signal>clicked()</signal>
        <receiver>ShowSimResultsForm</receiver>
        <slot>save_xml()</slot>
    </connection>
    <connection>
        <sender>saveTextButton</sender>
        <signal>clicked()</signal>
        <receiver>ShowSimResultsForm</receiver>
        <slot>save_text()</slot>
    </connection>
    <connection>
        <sender>pushButton3</sender>
        <signal>clicked()</signal>
        <receiver>ShowSimResultsForm</receiver>
        <slot>accept()</slot>
    </connection>
</connections>
<slots>
    <slot>save_xml()</slot>
    <slot>save_text()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
