####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: coasim_gui.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile coasim_gui.pro
qmake: 
	@$(QMAKE) -o Makefile coasim_gui.pro

dist: 
	@mkdir -p .obj/coasim_gui && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/coasim_gui/ && ( cd `dirname .obj/coasim_gui` && $(TAR) coasim_gui.tar coasim_gui && $(GZIP) coasim_gui.tar ) && $(MOVE) `dirname .obj/coasim_gui`/coasim_gui.tar.gz . && $(DEL_FILE) -r .obj/coasim_gui

mocclean:
